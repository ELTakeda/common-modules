                            <input type="number" name="" value="0" id="result1" disabled>
                        </div>
                        <div class="col-md-4 col-sm-4  col-xs-4">
                            <p>
                                {{ "mg IV/cycle"|t }}
                            </p>
                        </div>
                    </div>

                    <div class="row">
                        <div class="col-md-4 col-sm-4  col-xs-4 column2">
                            <p>{{ "V (vinblastina)"|t }}<sup>4</sup></p>
                        </div>
